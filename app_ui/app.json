{
    "expo": {
        "name": "Lemoo",
        "slug": "lemoo",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./src/assets/images/Android/ic_launcher_google_play.png",
        "scheme": "lemoo",
        "userInterfaceStyle": "automatic",
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./src/assets/images/Android/ic_launcher_google_play.png",
                "backgroundColor": "#ffffff"
            }
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./src/assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            "expo-secure-store",
            "expo-font",
            "expo-video",
            [
                "expo-splash-screen",
                {
                    "backgroundColor": "#ffffff",
                    "image": "./src/assets/images/Android/ic_launcher_google_play.png"
                }
            ],
            [
                "expo-image-picker",
                {
                    "photosPermission": "The app accesses your photos to let you share them with your friends."
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        }
    }
}
